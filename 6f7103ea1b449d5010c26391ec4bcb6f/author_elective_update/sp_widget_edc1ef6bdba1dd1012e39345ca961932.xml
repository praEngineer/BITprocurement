<?xml version="1.0" encoding="UTF-8"?><record_update table="sp_widget">
    <sp_widget action="DELETE">
        <category>custom</category>
        <client_script><![CDATA[function($scope,$rootScope) {
	/* widget controller */
	var c = this;

	$rootScope.$on('selectApp', function(event,data) {
console.log("cl approval body got event");
	
		c.server.get({
			action: 'getApp',
			appID: $rootScope.appID
		}).then(function(r) {
		
			c.data.number = r.data.app.number;
			c.data.short_description = r.data.app.short_description;
			c.data.appID = r.data.app.sys_id;
		});		
	});
}]]></client_script>
        <controller_as>c</controller_as>
        <css/>
        <data_table>sp_instance</data_table>
