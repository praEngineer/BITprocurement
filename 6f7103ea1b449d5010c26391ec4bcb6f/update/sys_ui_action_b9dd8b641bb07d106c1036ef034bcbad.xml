        <script><![CDATA[gs.addInfoMessage("Creating Procurement");
action.setRedirectURL(current);

var gr = new GlideRecord('x_dosny_bit_procur_bit_noncompetitive_small_purchase');
gr.initialize();
gr.requested_item = current.sys_id;
gr.request = current.request;

gr.request_requested_by = current.variables.requested_by;
gr.request_requested_for = current.variables.requested_for;
gr.request_name = current.variables.name;
gr.request_description = current.variables.description;
gr.request_justification = current.variables.justification;
gr.request_vendor = current.variables.vendor;
gr.request_estimated_cost = current.variables.estimated_cost;

if (current.variables.recurring_expense == 'Yes') {
	gr.request_recurring_expense = true;
} else {
	gr.request_recurring_expense = false;
}

gr.insert();]]></script>
