 }]]></link>
        <name>BIT Procurement Approval Body</name>
        <option_schema/>
        <public>true</public>
        <roles/>
        <script><![CDATA[(function() {
	/* populate the 'data' object */

	if (input && input.appID) {
	
		var app = new GlideRecord('x_dosny_bit_procur_bit_procurement_approval');
		if (app.get(input.appID)) {
			if (input.action == 'getApp') {
				data.app = {};
				$sp.getRecordValues(data.app, app, "number, short_description, sys_id");
			}
			else if (input.action == 'updateApp') { 			
				app.number = input.appNumber; 
				app.short_description = input.appBody; 			
				app.update(); 
			}
		}
	}
})();]]></script>
